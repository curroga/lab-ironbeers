<div>
  <h3>Bienvenidos a Random Cervezas</h3>
  <p>...</p>
  
    {{#each responseFromApi}}

        
          <div>
            <img src="{{this.image_url}}" alt="" width="50px">
          </div> 
          <div>
            {{this.name}}
          </div> 
          <div> 
            {{this.description}}
          </div> 
          <div>
            {{this.tagline}}
          </div>
          <div>
            <ul>Food Paring
              {{#each this.food_pairing}}
              <li>{{this}}</li> 
              {{/each}}           
            </ul>
          </div>
          <div>
            {{this.brewers_tips}}
          </div>
        


    {{/each}}
  
</div>